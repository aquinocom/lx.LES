<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="Sunburst Theme">
    <object name="lx_LES_templates"
        meta_type="Filesystem Directory View"
        directory="lx.LES:skins/templates" />
    <skin-path
        name="Sunburst Theme"
        based-on="Sunburst Theme">
        <layer
            name="lx_LES_templates"
            insert-after="custom"/>
    </skin-path>
    
    <object name="lx_LES_scripts"
        meta_type="Filesystem Directory View"
        directory="lx.LES:skins/scripts" />
    <skin-path
        name="Sunburst Theme"
        based-on="Sunburst Theme">
        <layer
            name="lx_LES_scripts"
            insert-after="custom"/>
    </skin-path>

    <object name="lx_LES_images"
        meta_type="Filesystem Directory View"
        directory="lx.LES:skins/images" />
    <skin-path
        name="Sunburst Theme"
        based-on="Sunburst Theme">
        <layer
            name="lx_LES_images"
            insert-after="custom"/>
    </skin-path>
</object>
